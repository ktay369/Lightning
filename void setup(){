void setup(){
	strokeWeight();
	background();
}
int startX = 0;
int startY = 150;
endX = 0;
endY = 150;
void draw(){
	stroke()
	while(endX>0){
	endX = (int)(Math.random()*9);
	endY = (int)(Math.random()*18)-9;
	line(startX, startY, endX, endY);
	startX = endX;
	startY = endY;
	}
}
void mousePressed(){
	startX = 0
	startY = 150
}